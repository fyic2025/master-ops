# Teelixir Environment Variables
# Copy to .env and fill in actual values

# BigCommerce Store
TEELIXIR_BC_STORE_HASH=your-store-hash
TEELIXIR_BC_ACCESS_TOKEN=your-access-token
TEELIXIR_BC_CLIENT_ID=your-client-id

# Supabase Database
TEELIXIR_SUPABASE_URL=https://your-project.supabase.co
TEELIXIR_SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
TEELIXIR_SUPABASE_ANON_KEY=your-anon-key

# Xero Integration
XERO_TEELIXIR_CLIENT_ID=your-xero-client-id
XERO_TEELIXIR_CLIENT_SECRET=your-xero-client-secret
